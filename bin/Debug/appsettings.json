{
    "AllowedHosts": "*",
    "ConnectionStrings": {
        "devDB": "Server=localhost\\SQLEXPRESS;Database=AdventureWorks2022;Trusted_Connection=True;TrustServerCertificate=True;"


    },
    "Serilog": {
        "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.Seq" ],
        "MinimumLevel": {
            "Default": "Information",
            "Override": {
                "Microsoft": "Information"
            }
        },
        "WriteTo": [
            { "Name": "Console" },
            {
                "Name": "Seq",
                "Args": { "serverUrl": "http://eshop-seq:5341" }
            }

        ],
        "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ]
    }
}

